# .env file for GitLab to GitHub migration script

# GitLab API token with read_api, read_repository permissions
GITLAB_TOKEN=<YOUR_GITLAB_ACCESS_TOKEN>

# GitHub organization name where repositories will be created
GITHUB_ORG=<YOUR_GITHUB_ORG>

# Path to CSV file containing GitLab repo URLs and slugs
CSV_FILE=gitlab-repos.csv

# Directory to clone repositories into
REPOS_DIR=<YOUR_REPOS_DIR>

# Temporary file for CSV updates
TMP_FILE=<YOUR_TMP_FILE>/temp.csv

# Git author information for cleaning commit history
GIT_AUTHOR_NAME=<YOUR_GIT_AUTHOR_NAME>
GIT_AUTHOR_EMAIL=<YOUR_GIT_AUTHOR_EMAIL>

# Log level: ERROR, WARN, INFO, or DEBUG
LOG_LEVEL=INFO
